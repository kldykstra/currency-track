            "hide": false,
            "rawQuery": true,
            "rawSql": "SELECT\n  conversion_date as time,\n  currency_code as metric,\n  MAX(conversion_rate) as value\nFROM conversion_rates\nWHERE currency_code IN (\n  SELECT unnest(\n    CASE\n      WHEN CARDINALITY(ARRAY[$CurrencyCode]) > 1 THEN ARRAY[$CurrencyCode]\n      WHEN ARRAY[$CurrencyCode] != ARRAY[''] THEN ARRAY[$CurrencyCode]\n      ELSE ARRAY['USD', 'CAD']\n    END\n    )\n)\nGROUP BY\n  conversion_date, currency_code",
